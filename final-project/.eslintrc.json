{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": 0,
        "no-underscore-dangle": 0, 
        "class-methods-use-this": 0,
        "import/extensions": 0,
        "no-console": 0,
        "no-unused-expressions": 0,
        "consistent-return": 0,
        "no-param-reassign": 0
    }
}
